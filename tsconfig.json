{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "types": ["node", "jest"],
    "noEmitOnError": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["**/*.test.ts","**/*.spec.ts"]
}